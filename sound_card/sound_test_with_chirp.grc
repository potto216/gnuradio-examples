- name: epy_block_0
  id: epy_block
  parameters:
    _source_code: "\"\"\"\nEmbedded Python Blocks:\n\nEach time this file is saved,\
      \ GRC will instantiate the first class it finds\nto get ports and parameters\
      \ of your block. The arguments to __init__  will\nbe the parameters. All of\
      \ them are required to have default values!\n\"\"\"\n\n\n#!/usr/bin/env python\n\
      # -*- coding: utf-8 -*-\n# \n# GNU Radio Python block for integer input integration\
      \ with float output\n#\n\nimport numpy as np\nfrom gnuradio import gr\n\nclass\
      \ int_integrator_ff(gr.sync_block):\n    \"\"\"\n    Integrates an integer input\
      \ and outputs as a float.\n    \"\"\"\n\n    def __init__(self, integer_modulus=4096):\n\
      \        gr.sync_block.__init__(\n            self,\n            name='Integer\
      \ Integrator',\n            in_sig=[np.int32],\n            out_sig=[np.float32]\n\
      \        )\n        # Initialize the accumulator\n        self.accumulator =\
      \ 0.0\n        self.integer_modulus=integer_modulus\n\n    def work(self, input_items,\
      \ output_items):\n        in0 = input_items[0]    # Integer input\n        out\
      \ = output_items[0]   # Float output\n\n        # Integrate the input signal\n\
      \        for i in range(len(in0)):\n            self.accumulator += in0[i]\n\
      \            self.accumulator %= self.integer_modulus\n            out[i] =\
      \ self.accumulator\n\n        return len(output_items[0])\n\n"
    affinity: ''
    alias: ''
    comment: ''
    integer_modulus: '4096'
    maxoutbuf: '0'
    minoutbuf: '0'
  states:
    _io_cache: ('Integer Integrator', 'int_integrator_ff', [('integer_modulus', '4096')],
      [('0', 'int', 1)], [('0', 'float', 1)], '\n    Integrates an integer input and
      outputs as a float.\n    ', ['integer_modulus'])
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [248, 232.0]
    rotation: 0
    state: enabled
